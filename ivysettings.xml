<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="main" />
  <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
	<credentials host="192.168.0.3" realm="Artifactory Realm" username="${user}" passwd="${password}"/>
  <resolvers>
    <chain name="main">
      <url name="public" m2compatible="false">
        <artifact pattern="http://192.168.0.3:8081/artifactory/libs-release-local/[organization]/[module]/[revision]/[type]s/[module](-[classifier])-[revision].[ext]" />
        <ivy pattern="http://192.168.0.3:8081/artifactory/libs-release-local/[organization]/[module]/[revision]/[type]s/ivy-[revision].xml" />
      </url>
    </chain>
  </resolvers>
</ivy-settings>
